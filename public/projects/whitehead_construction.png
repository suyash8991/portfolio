<svg width="800" height="450" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGradient2" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0f172a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1e293b;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="accentGradient2" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#f59e0b;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ef4444;stop-opacity:1" />
    </linearGradient>
  </defs>

  <!-- Background -->
  <rect width="800" height="450" fill="url(#bgGradient2)"/>

  <!-- Decorative elements -->
  <circle cx="100" cy="100" r="60" fill="#f59e0b" opacity="0.08"/>
  <circle cx="700" cy="350" r="80" fill="#ef4444" opacity="0.08"/>

  <!-- Building/Construction icon -->
  <g transform="translate(300, 100)">
    <!-- Building structure -->
    <rect x="20" y="60" width="160" height="120" fill="none" stroke="#f59e0b" stroke-width="3" opacity="0.6"/>
    <line x1="20" y1="100" x2="180" y2="100" stroke="#f59e0b" stroke-width="2" opacity="0.4"/>
    <line x1="20" y1="140" x2="180" y2="140" stroke="#f59e0b" stroke-width="2" opacity="0.4"/>

    <!-- Windows -->
    <rect x="40" y="70" width="25" height="25" fill="#ef4444" opacity="0.3"/>
    <rect x="85" y="70" width="25" height="25" fill="#f59e0b" opacity="0.3"/>
    <rect x="130" y="70" width="25" height="25" fill="#ef4444" opacity="0.3"/>

    <rect x="40" y="110" width="25" height="25" fill="#f59e0b" opacity="0.3"/>
    <rect x="85" y="110" width="25" height="25" fill="#ef4444" opacity="0.3"/>
    <rect x="130" y="110" width="25" height="25" fill="#f59e0b" opacity="0.3"/>

    <rect x="40" y="150" width="25" height="25" fill="#ef4444" opacity="0.3"/>
    <rect x="85" y="150" width="25" height="25" fill="#f59e0b" opacity="0.3"/>
    <rect x="130" y="150" width="25" height="25" fill="#ef4444" opacity="0.3"/>

    <!-- Hierarchical structure indicators -->
    <circle cx="220" cy="80" r="6" fill="#f59e0b"/>
    <circle cx="250" cy="80" r="6" fill="#ef4444"/>
    <circle cx="235" cy="100" r="5" fill="#f59e0b"/>
    <line x1="220" y1="80" x2="235" y2="100" stroke="#f59e0b" stroke-width="2" opacity="0.6"/>
    <line x1="250" y1="80" x2="235" y2="100" stroke="#ef4444" stroke-width="2" opacity="0.6"/>
  </g>

  <!-- Title -->
  <text x="400" y="280" font-family="Arial, sans-serif" font-size="32" font-weight="bold" fill="url(#accentGradient2)" text-anchor="middle">
    Whitehead Construction
  </text>

  <!-- Subtitle -->
  <text x="400" y="315" font-family="Arial, sans-serif" font-size="18" fill="#94a3b8" text-anchor="middle">
    Architect of Digital Realms
  </text>

  <!-- Tech stack badges -->
  <g transform="translate(180, 350)">
    <rect x="0" y="0" width="90" height="30" rx="15" fill="#f59e0b" opacity="0.2"/>
    <text x="45" y="20" font-family="Arial, sans-serif" font-size="14" fill="#f59e0b" text-anchor="middle">React 18</text>

    <rect x="100" y="0" width="100" height="30" rx="15" fill="#ef4444" opacity="0.2"/>
    <text x="150" y="20" font-family="Arial, sans-serif" font-size="14" fill="#ef4444" text-anchor="middle">Express.js</text>

    <rect x="210" y="0" width="90" height="30" rx="15" fill="#f59e0b" opacity="0.2"/>
    <text x="255" y="20" font-family="Arial, sans-serif" font-size="14" fill="#f59e0b" text-anchor="middle">SQLite3</text>

    <rect x="310" y="0" width="130" height="30" rx="15" fill="#ef4444" opacity="0.2"/>
    <text x="375" y="20" font-family="Arial, sans-serif" font-size="14" fill="#ef4444" text-anchor="middle">Tailwind CSS</text>
  </g>

  <!-- Placeholder indicator -->
  <text x="400" y="420" font-family="Arial, sans-serif" font-size="12" fill="#64748b" text-anchor="middle" opacity="0.6">
    Placeholder - Replace with actual screenshot
  </text>
</svg>
